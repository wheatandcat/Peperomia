    "@react-native-community/eslint-config": "^1.0.0",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addon-ondevice-knobs": "^5.3.0",
    "@storybook/addon-ondevice-notes": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/react-native": "^5.3.0",
    "@storybook/react-native-server": "^5.3.0",
    "@storybook/theming": "^5.3.0",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.5",
    "@types/jasmine": "^3.5.1",
    "@types/jest": "^25.1.3",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/react-native-status-bar-height": "^2.1.0",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.1.0",
    "codecov": "^3.6.5",
    "danger": "^9.2.10",
    "danger-plugin-istanbul-coverage": "^1.6.2",
    "detox": "^15.0.0",
    "detox-expo-helpers": "^0.6.0",
    "emotion-theming": "^10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-native": "^3.8.1",
    "expo-detox-hook": "^1.0.10",
    "firebase-mock": "^2.3.2",
    "husky": "^4.0.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^37.0.0",
    "jest-expo-enzyme": "^1.0.0",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^9.2.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "prop-types": "^15.6.2",
    "react-dom": "16.9.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.1.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro"
      }
    }
  }
}
