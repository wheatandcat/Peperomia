{
  "name": "peperomia",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "app:copy": "cp -r App.app.tsx App.tsx",
    "storybook:copy": "cp -r App.storybook.tsx App.tsx",
    "start": "npm run app:copy && npm run development:config && expo start -c",
    "android": "npm run app:copy && expo start --android",
    "ios": "npm run app:copy && expo start --ios",
    "eject": "expo eject",
    "test": "jest ./src/***/*.test.ts",
    "storybook": "npm run storybook:copy && npm run development:config && expo start",
    "prestorybook": "rnstl",
    "storybook-server": "start-storybook",
    "e2e": "rimraf ./screenshots && detox test --configuration ios.sim && open ./screenshots",
    "eslint": "eslint -c ./.eslintrc.json 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix -c ./.eslintrc.json 'src/**/*.{ts,tsx}'",
    "development:config": "sh ./setupDevelopment.sh",
    "release:config": "sh ./setupRelease.sh",
    "build:ios": "npm run release:config && expo build:ios",
    "build:android": "npm run release:config && expo build:android"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.1.2",
    "@types/color": "^3.0.0",
    "@types/expo": "^32.0.13",
    "@types/firebase": "^3.2.1",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.60.2",
    "@types/react-native-loading-spinner-overlay": "^0.5.1",
    "@types/react-native-sortable-list": "0.0.7",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^3.0.1",
    "@types/styled-components": "^4.1.18",
    "@types/uuid": "^3.4.4",
    "color": "^3.1.0",
    "expo": "^33.0.0",
    "expo-camera": "~5.0.1",
    "expo-constants": "~5.0.1",
    "expo-font": "~5.0.1",
    "expo-image-manipulator": "~5.0.1",
    "expo-image-picker": "~5.0.2",
    "expo-permissions": "~5.0.1",
    "expo-sqlite": "~5.0.1",
    "firebase": "^6.2.4",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-app-intro-slider": "^2.0.1",
    "react-native-easy-grid": "^0.2.1",
    "react-native-elements": "^1.1.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-paper": "^2.1.3",
    "react-native-root-toast": "^3.0.2",
    "react-native-sortable-list": "^0.0.23",
    "react-native-status-bar-height": "^2.3.1",
    "react-native-svg": "^9.5.3",
    "react-native-swipeout": "^2.3.6",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-ui-lib": "^3.3.248",
    "react-native-view-shot": "^3.0.0",
    "react-navigation": "^3.11.0",
    "sentry-expo": "^1.11.2",
    "styled-components": "^4.1.3",
    "tslib": "^1.9.3",
    "typescript": "^3.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-ondevice-knobs": "^5.1.9",
    "@storybook/addon-ondevice-notes": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react-native": "5.2.0-alpha.36",
    "@storybook/react-native-server": "5.2.0-alpha.36",
    "@storybook/theming": "^5.1.9",
    "@types/detox": "^12.8.2",
    "@types/jasmine": "^3.3.14",
    "@types/jest": "^24.0.15",
    "@types/react-native-status-bar-height": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.9.0",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-plugin-inline-dotenv": "^1.3.3",
    "babel-preset-expo": "^5.2.0",
    "detox": "~12.3.0",
    "detox-expo-helpers": "^0.6.0",
    "emotion-theming": "^10.0.14",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-native": "^3.7.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "prop-types": "^15.6.2",
    "react-dom": ">=16.3.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(expo)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "jest",
    "specs": ""
  }
}
