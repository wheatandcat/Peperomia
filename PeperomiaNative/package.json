{
  "name": "peperomia",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "app:copy": "cp -r App.app.tsx App.tsx",
    "storybook:copy": "cp -r App.storybook.tsx App.tsx",
    "start": "npm run app:copy && npm run development:config && expo start --no-minify",
    "android": "npm run app:copy && expo start --android",
    "ios": "npm run app:copy && expo start --ios",
    "eject": "expo eject",
    "test": "jest ./src/***/*.test.ts",
    "storybook": "npm run storybook:copy && npm run development:config && expo start --no-minify",
    "prestorybook": "rnstl",
    "storybook-server": "start-storybook",
    "e2e": "detox test --configuration ios.sim",
    "eslint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "development:config": "sh ./setupDevelopment.sh",
    "release:config": "sh ./setupRelease.sh",
    "build:ios": "npm run release:config && expo build:ios",
    "build:android": "npm run release:config && expo build:android"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.1.2",
    "@specter/client": "^0.0.22",
    "@specter/specter": "^0.0.22",
    "@types/color": "^3.0.0",
    "@types/compare-versions": "^3.3.0",
    "@types/expo": "^33.0.1",
    "@types/firebase": "^3.2.1",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.61.4",
    "@types/react-native-app-intro-slider": "^3.0.0",
    "@types/react-native-calendars": "^1.20.7",
    "@types/react-native-datepicker": "^1.7.0",
    "@types/react-native-loading-spinner-overlay": "^0.5.1",
    "@types/react-native-sortable-list": "0.0.8",
    "@types/react-native-swipe-gestures": "^1.0.0",
    "@types/react-native-vector-icons": "^6.4.4",
    "@types/react-navigation": "^3.4.0",
    "@types/styled-components": "^4.1.18",
    "@types/uuid": "^3.4.6",
    "color": "^3.1.0",
    "compare-versions": "^3.5.0",
    "dayjs": "^1.8.18",
    "expo": "^36.0.0",
    "expo-camera": "~8.0.0",
    "expo-constants": "~8.0.0",
    "expo-font": "~8.0.0",
    "expo-google-app-auth": "^8.0.0",
    "expo-google-sign-in": "~8.0.0",
    "expo-image-manipulator": "~8.0.0",
    "expo-image-picker": "~8.0.0",
    "expo-permissions": "~8.0.0",
    "expo-sqlite": "~8.0.0",
    "firebase": "^7.6.1",
    "primitive": "../primitive",
    "query-string": "^6.8.3",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-appearance": "~0.3.1",
    "react-native-calendars": "^1.221.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-easy-grid": "^0.2.2",
    "react-native-elements": "^1.2.7",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-reanimated": "~1.4.0",
    "react-native-root-toast": "^3.0.2",
    "react-native-sortable-list": "^0.0.23",
    "react-native-status-bar-height": "^2.3.1",
    "react-native-svg": "9.13.3",
    "react-native-svg-animated-linear-gradient": "^0.3.3",
    "react-native-swipe-gestures": "^1.0.4",
    "react-native-swipe-list-view": "^2.1.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-webview": "7.4.3",
    "react-navigation": "^4.0.10",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.7.0",
    "sentry-expo": "^2.0.1",
    "styled-components": "^4.1.3",
    "tslib": "^1.9.3",
    "typescript": "^3.7.5",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.6",
    "@react-native-community/eslint-plugin": "^1.0.0",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addon-ondevice-knobs": "^5.3.0",
    "@storybook/addon-ondevice-notes": "^5.3.0",
    "@storybook/addons": "^5.3.13",
    "@storybook/react-native": "^5.3.0",
    "@storybook/react-native-server": "^5.3.0",
    "@storybook/theming": "^5.3.0",
    "@types/detox": "^14.5.2",
    "@types/jasmine": "^3.5.1",
    "@types/jest": "^24.0.25",
    "@types/react-native-status-bar-height": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-preset-expo": "^8.0.0",
    "detox": "^15.0.0",
    "detox-expo-helpers": "^0.6.0",
    "emotion-theming": "^10.0.27",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-native": "^3.8.1",
    "expo-detox-hook": "^1.0.10",
    "husky": "^4.0.7",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "lint-staged": "^9.2.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.2",
    "react-dom": "16.9.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(expo)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro"
      }
    }
  }
}
