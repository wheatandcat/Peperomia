    "react-native-webview": "8.1.1",
    "react-responsive": "^8.0.3",
    "sentry-expo": "^2.0.3",
    "styled-components": "^4.1.3",
    "tslib": "^1.9.3",
    "typescript": "^3.8.3",
    "uuid": "^3.3.3"
