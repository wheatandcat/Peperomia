{
  "name": "peperomia",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "app:copy": "cp -r App.app.tsx App.tsx && cp -r src/lib/system/firebase/_product.ts src/lib/system/firebase/index.ts",
    "storybook:copy": "cp -r App.storybook.tsx App.tsx && cp -r src/lib/system/firebase/_product.ts src/lib/system/firebase/index.t",
    "test:copy": "cp -r src/lib/system/firebase/_mock.ts src/lib/system/firebase/index.ts",
    "start": "npm run app:copy && npm run development:config && expo start --no-minify -c",
    "android": "npm run app:copy && expo start --android",
    "ios": "npm run app:copy && expo start --ios",
    "eject": "expo eject",
    "test": "npm run test:copy && jest",
    "test:coverage": "npm run test:copy && jest --coverage",
    "storybook": "npm run storybook:copy && npm run development:config && expo start --no-minify",
    "prestorybook": "rnstl",
    "storybook-server": "start-storybook",
    "e2e": "detox test --configuration ios.sim",
    "eslint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "eslint:fix": "eslint --fix -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "development:config": "sh ./setupDevelopment.sh",
    "release:config": "sh ./setupRelease.sh",
    "build:ios": "npm run release:config && expo build:ios",
    "build:android": "npm run release:config && expo build:android",
    "danger": "danger local",
    "danger:ci": "danger ci"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.5.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/bottom-tabs": "^5.2.5",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "@specter/client": "^0.0.22",
    "@specter/specter": "^0.0.22",
    "@types/color": "^3.0.0",
    "@types/compare-versions": "^3.3.0",
    "@types/expo": "^33.0.1",
    "@types/firebase": "^3.2.1",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-native-app-intro-slider": "^3.0.1",
    "@types/react-native-calendars": "^1.20.7",
    "@types/react-native-datepicker": "^1.7.0",
    "@types/react-native-loading-spinner-overlay": "^0.5.1",
    "@types/react-native-sortable-list": "0.0.8",
    "@types/react-native-swipe-gestures": "^1.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/styled-components": "^4.1.18",
    "@types/uuid": "^3.4.6",
    "color": "^3.1.0",
    "compare-versions": "^3.5.0",
    "dayjs": "^1.8.18",
    "expo": "^37.0.0",
    "expo-apple-authentication": "~2.1.0",
    "expo-camera": "~8.2.0",
    "expo-constants": "~9.0.0",
    "expo-device": "~2.1.0",
