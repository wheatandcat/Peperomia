{
  "name": "peperomia",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "app": "cp -r App.app.tsx App.tsx && cp -r src/lib/system/firebase/_product.ts src/lib/system/firebase/index.ts",
    "app:copy": "cp -r App.app.tsx App.tsx && cp -r src/lib/system/firebase/_product.ts src/lib/system/firebase/index.ts",
    "storybook:copy": "cp -r App.storybook.tsx App.tsx && cp -r src/lib/system/firebase/_product.ts src/lib/system/firebase/index.ts",
    "test:copy": "cp -r src/lib/system/firebase/_mock.ts src/lib/system/firebase/index.ts",
    "start": "npm run app:copy && npm run development:config && expo start --no-minify -c",
    "android": "npm run app:copy && expo start --android",
    "ios": "npm run app:copy && expo start --ios",
    "eject": "expo eject",
    "test": "npm run test:copy && jest",
    "test:coverage": "npm run test:copy && jest --coverage",
    "storybook": "npm run storybook:copy && npm run development:config && expo start --no-minify",
    "prestorybook": "rnstl",
    "storybook-server": "start-storybook",
    "e2e": "detox test --configuration ios.sim",
    "lint": "eslint --ext .js,.ts,.tsx,.gql,.graphql 'src/**'",
    "lint:fix": "eslint --fix .js,.ts,.tsx 'src/**'",
    "development:config": "sh ./setupDevelopment.sh",
    "release:config": "sh ./setupRelease.sh",
    "build:ios": "npm run app:copy && npm run release:config && expo build:ios",
    "build:android": "npm run app:copy && npm run release:config && expo build:android",
    "danger": "danger local",
    "danger:ci": "danger ci",
    "tsc": "tsc",
    "download:schema.graphqls": "curl -L -O https://raw.githubusercontent.com/wheatandcat/PeperomiaBackend/master/graph/schema.graphqls",
    "codegen": "npm run download:schema.graphqls && graphql-codegen",
    "codegen:lint:fix": "eslint --fix ./src/queries/api/index.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@expo/react-native-action-sheet": "^3.5.0",
    "@react-native-community/async-storage": "~1.12.0",
    "@react-native-community/datetimepicker": "3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "@specter/client": "^0.5.5",
    "@specter/specter": "^0.5.5",
    "@types/color": "^3.0.0",
    "@types/compare-versions": "^3.3.0",
    "@types/deep-equal": "^1.0.1",
    "@types/firebase": "^3.2.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-native-app-link": "^1.0.0",
    "@types/react-native-calendars": "^1.20.11",
    "@types/react-native-loading-spinner-overlay": "^0.5.1",
    "@types/react-native-sortable-list": "0.0.8",
    "@types/react-native-swipe-gestures": "^1.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/uuid": "^3.4.6",
    "color": "^3.1.0",
    "compare-versions": "^3.5.0",
    "dayjs": "^1.8.18",
    "deep-equal": "^2.0.5",
    "expo": "^40.0.0",
    "expo-apple-authentication": "~2.2.2",
    "expo-camera": "~9.1.0",
    "expo-constants": "~9.3.3",
    "expo-device": "~2.4.0",
    "expo-firebase-analytics": "~2.6.0",
    "expo-firebase-core": "~1.3.0",
    "expo-font": "~8.4.0",
    "expo-google-app-auth": "^8.0.0",
    "expo-google-sign-in": "~8.4.0",
    "expo-image-manipulator": "~8.4.0",
    "expo-image-picker": "~9.2.0",
    "expo-intent-launcher": "~8.4.0",
    "expo-linking": "~2.0.0",
    "expo-notifications": "~0.8.2",
    "expo-permissions": "~10.0.0",
    "expo-screen-orientation": "~2.1.0",
    "expo-splash-screen": "~0.8.1",
    "expo-sqlite": "~8.5.0",
    "firebase": "7.9.0",
    "graphql": "^15.3.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.11.0",
    "peperomia-util": "^0.0.17",
    "query-string": "^6.8.3",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-app-link": "^1.0.0",
    "react-native-appearance": "~0.3.3",
    "react-native-calendars": "^1.403.0",
    "react-native-easy-grid": "^0.2.2",
    "react-native-elements": "3.1.0",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-modal": "^11.5.6",
    "react-native-modal-datetime-picker": "^8.9.3",
    "react-native-reanimated": "~1.13.0",
    "react-native-root-toast": "^3.0.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.0",
    "react-native-sortable-list": "^0.0.23",
    "react-native-status-bar-height": "^2.3.1",
    "react-native-svg": "12.1.0",
    "react-native-svg-animated-linear-gradient": "^0.4.0",
    "react-native-swipe-gestures": "^1.0.4",
    "react-native-swipe-list-view": "^2.1.3",
    "react-native-typescript-transformer": "^1.2.10",
    "react-native-webview": "11.0.0",
    "react-responsive": "^8.0.3",
    "sentry-expo": "^2.0.3",
    "tslib": "^1.9.3",
    "typescript": "~4.0.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.18.0",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@graphql-eslint/eslint-plugin": "^0.7.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@storybook/addon-actions": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-links": "^5.3.0",
    "@storybook/addon-ondevice-knobs": "^5.3.0",
    "@storybook/addon-ondevice-notes": "^5.3.0",
    "@storybook/addons": "^5.3.0",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@storybook/theming": "^5.3.0",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.5",
    "@types/jasmine": "^3.5.1",
    "@types/jest": "^25.1.3",
    "@types/jest-plugin-context": "^2.9.2",
    "@types/react-native-status-bar-height": "^2.1.0",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "8.3.0",
    "codecov": "^3.6.5",
    "danger": "^9.2.10",
    "danger-plugin-istanbul-coverage": "^1.6.2",
    "detox": "^16.6.0",
    "detox-expo-helpers": "^0.6.0",
    "emotion-theming": "^10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "expo-detox-hook": "^1.0.10",
    "firebase-mock": "^2.3.2",
    "husky": "^4.0.7",
    "hygen": "^6.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^40.0.0",
    "jest-expo-enzyme": "^1.0.2",
    "jest-plugin-context": "^2.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^2.2.1",
    "prop-types": "^15.6.2",
    "react-dom": "16.13.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.1.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/stories.*",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn tsc && lint-staged"
    }
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "bin/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro"
      }
    }
  }
}
